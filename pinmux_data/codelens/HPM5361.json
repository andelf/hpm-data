{
    "ioc": {
        "type": "baseioc",
        "default": {
            "name": "HPM_IOC"
        },
        "PMIC": {
            "name": "HPM_PIOC"
        },
        "BATT": {
            "name": "HPM_BIOC"
        }
    },
    "gpio": {
        "type": "basegpio",
        "gpioType":{
            "key": "select",
            "value": {
                "-1": "HPM_PGPIO",
                "1": "HPM_GPIO0",
                "2": "HPM_FGPIO"
            }
        },
        "select": {
            "1": "gpiom_soc_gpio0",
            "2": "gpiom_core0_fast"
        }
    },
    "trgm": {
        "type": "basetrgm",
        "trgmType": {
            "0": "HPM_TRGM0"
        } 
    },
    "src_header_annotation": "\n * Note:\n *   PY and PZ IOs: if any SOC pin function needs to be routed to these IOs,\n *  besides of IOC, PIOC/BIOC needs to be configured SOC_GPIO_X_xx, so that\n *  expected SoC function can be enabled on these IOs.\n *"
}