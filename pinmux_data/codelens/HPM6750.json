{
    "ioc": {
        "type": "baseioc",
        "default": {
            "name": "HPM_IOC"
        },
        "PMIC": {
            "name": "HPM_PIOC"
        },
        "BATT": {
            "name": "HPM_BIOC"
        }
    },
    "gpio": {
        "type": "basegpio",
        "gpioType":{
            "key": "select",
            "value": {
                "-2": "HPM_BGPIO",
                "-1": "HPM_PGPIO",
                "1": "HPM_GPIO0",
                "2": "HPM_GPIO1",
                "3": "HPM_FGPIO",
                "4": "HPM_FGPIO"
            }
        },
        "select": {
            "1": "gpiom_soc_gpio0",
            "2": "gpiom_soc_gpio1",
            "3": "gpiom_core0_fast",
            "4": "gpiom_core1_fast"
        }
    },
    "trgm": {
        "type": "basetrgm",
        "trgmType": {
            "0": "HPM_TRGM0",
            "1": "HPM_TRGM1",
            "2": "HPM_TRGM2",
            "3": "HPM_TRGM3"
        } 
    },
    "src_header_annotation": "\n * Note:\n *   PY and PZ IOs: if any SOC pin function needs to be routed to these IOs,\n *  besides of IOC, PIOC/BIOC needs to be configured SOC_GPIO_X_xx, so that\n *  expected SoC function can be enabled on these IOs.\n *"
}