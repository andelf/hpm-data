            "0": "gpio_disable_pin_interrupt({{gpioType}}, GPIO_IE_GPIO${group}, ${pin})"
        },
        "interruptConfig": "gpio_config_pin_interrupt({{gpioType}}, GPIO_DI_GPIO${group}, ${pin}, {{intrConfig}})",
